<p id="notice"><%= notice %></p>

<h1>Incomplete Puzzles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Incompletepuzzle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incomplete_puzzles.each do |incomplete_puzzle| %>
      <tr>
        <td><%= incomplete_puzzle.user_id %></td>
        <td><%= incomplete_puzzle.incompletePuzzle %></td>
        <td><%= link_to 'Show', incomplete_puzzle %></td>
        <td><%= link_to 'Edit', edit_incomplete_puzzle_path(incomplete_puzzle) %></td>
        <td><%= link_to 'Destroy', incomplete_puzzle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Incomplete Puzzle', new_incomplete_puzzle_path %>
